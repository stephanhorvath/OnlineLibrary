<p id="notice"><%= notice %></p>

<h1>Booking Clerks</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Address line 1</th>
      <th>Address line 2</th>
      <th>Town</th>
      <th>Post code</th>
      <th>Tel no</th>
      <th>Email</th>
      <th>Password digest</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @booking_clerks.each do |booking_clerk| %>
      <tr>
        <td><%= booking_clerk.first_name %></td>
        <td><%= booking_clerk.last_name %></td>
        <td><%= booking_clerk.address_line_1 %></td>
        <td><%= booking_clerk.address_line_2 %></td>
        <td><%= booking_clerk.town %></td>
        <td><%= booking_clerk.post_code %></td>
        <td><%= booking_clerk.tel_no %></td>
        <td><%= booking_clerk.email %></td>
        <td><%= booking_clerk.password_digest %></td>
        <td><%= booking_clerk.type %></td>
        <td><%= link_to 'Show', booking_clerk %></td>
        <td><%= link_to 'Edit', edit_booking_clerk_path(booking_clerk) %></td>
        <td><%= link_to 'Destroy', booking_clerk, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Booking Clerk', new_booking_clerk_path %>
