<p id="notice"><%= notice %></p>

<h1>Membership Clerks</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Address line 1</th>
      <th>Address line 2</th>
      <th>Town</th>
      <th>Post code</th>
      <th>Tel no</th>
      <th>Email</th>
      <th>Password digest</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @membership_clerks.each do |membership_clerk| %>
      <tr>
        <td><%= membership_clerk.first_name %></td>
        <td><%= membership_clerk.last_name %></td>
        <td><%= membership_clerk.address_line_1 %></td>
        <td><%= membership_clerk.address_line_2 %></td>
        <td><%= membership_clerk.town %></td>
        <td><%= membership_clerk.post_code %></td>
        <td><%= membership_clerk.tel_no %></td>
        <td><%= membership_clerk.email %></td>
        <td><%= membership_clerk.password_digest %></td>
        <td><%= membership_clerk.type %></td>
        <td><%= link_to 'Show', membership_clerk %></td>
        <td><%= link_to 'Edit', edit_membership_clerk_path(membership_clerk) %></td>
        <td><%= link_to 'Destroy', membership_clerk, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Membership Clerk', new_membership_clerk_path %>
